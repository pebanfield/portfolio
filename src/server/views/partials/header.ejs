<div id="homeHeader">
  <div id="pebLogo">
   <a href=".">
    <img src="/img/pebLogo.gif"></img>
    <span>P.E. Banfield </span> 
   </a>
  </div>
  <div id="headerBg"></div>
  <div id="nav">
    <ul id="navItems">
      <% navItems.forEach(function(item) { %>
        <%if (activeItem.name === item.name) { %>
          <li><a class="activeNav" href="/<%= item.name %>"><%= item.name %></a></li>
        <% } else { %>
          <li><a class="nav" href="/<%= item.name %>"><%= item.name %></a></li>
        <% } %>
        <%if (item.name != 'home') { %>
          <li>|</li>
        <% } %>
       <% }); %>
    </ul>
  </div>
</div>